{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    },
    "Endpoints": {
      "UnSecureApi": {
        "Url": "http://localhost:5000",
        "Protocols": "Http1AndHttp2"
      },
      "SecureApi": {
        "Url": "https://localhost:5001",
        "Protocols": "Http2"
      },
      "Grpc": {
        "Url": "https://localhost:5002",
        "Protocols": "Http2"
      }
    }
  }
}
